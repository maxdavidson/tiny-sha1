{
  "name": "tiny-sha1",
  "version": "0.2.1",
  "description": "Tiny SHA-1 hasher for Node and browsers",
  "author": "Max Davidson <davidsonmax@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxdavidson/tiny-sha1.git"
  },
  "main": "dist/tiny-sha1.js",
  "module": "dist/tiny-sha1.es.js",
  "jsnext:main": "dist/tiny-sha1.es.js",
  "typings": "typings.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">=4.5.0",
    "npm": ">=2.15.9"
  },
  "keywords": [
    "sha1",
    "sha-1",
    "hash",
    "digest"
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "npm run build -- -w",
    "clean": "rimraf dist coverage .nyc_output",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test nyc ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "prebuild": "npm run clean",
    "posttest": "npm run lint",
    "preversion": "npm test",
    "prepublish": "npm run build",
    "postversion": "git push && git push --tags && npm publish"
  },
  "files": [
    "typings.d.ts",
    "dist"
  ],
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.14.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-preset-latest": "^6.14.0",
    "babel-register": "^6.14.0",
    "coveralls": "^2.11.13",
    "cross-env": "^3.0.0",
    "eslint": "^3.8.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.0.1",
    "nyc": "^10.0.0",
    "promisify-node": "^0.4.0",
    "rimraf": "^2.5.2",
    "rollup": "^0.36.0",
    "rollup-plugin-babel": "^2.6.1"
  }
}
